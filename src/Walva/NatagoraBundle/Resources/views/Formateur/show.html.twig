{% extends "WalvaNatagoraBundle:Formateur:layout.html.twig" %}

{% block breadcrumb %}
{{ parent() }}
<li class="active">Show</li>
{% endblock %}



{% block body -%}
<h1>Formateur : {{entity.prenom}} {{entity.nom}}</h1>

<table class="table table-striped">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ entity.prenom }}</td>
        </tr>
        <tr>
            <th>Mail</th>
            <td>{{ entity.mail }}</td>
        </tr>
        <tr>
            <th>Gsm</th>
            <td>{{ entity.gsm }}</td>
        </tr>
        <tr>
            <th>Tel</th>
            <td>{{ entity.tel }}</td>
        </tr>

    </tbody>
</table>


{% endblock %}

    {% block container_header_right %}

<div class="btn-group">
    <a class="btn " href="{{ path('formateur') }}">
        <i class="icon-chevron-left"></i> 
        Back to the list
    </a>
    <a class="btn " href="{{ path('formateur_edit', { 'id': entity.id }) }}">
        <i class="icon-pencil"></i> Edit</a>
</a>

</div>
{% endblock %}