{% extends "WalvaNatagoraBundle:Evenement:layout.html.twig" %}

{% block breadcrumb %}
{{ parent() }}
<li class="active">Creation</li>
{% endblock %}


{% block body -%}
<h1>Evenement creation</h1>
 <hr/>
<form class="form-horizontal" action="{{ path('evenement_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form, {'form_type': 'horizontal'}) }}
        <div class="form-actions">
        <button type="submit" class="btn">Create</button>
    </div>
</form>

{% endblock %}


    {% block container_header_right %}
<div class="btn-group">
    <a class="btn " href="{{ path('evenement') }}">
        <i class="icon-chevron-left"></i> 
        Back to the list
    </a>
</div>
{% endblock %}